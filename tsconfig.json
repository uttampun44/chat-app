{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/pages/*": [
        "./app/pages/*"
      ],
      "@/components/*": [
        "./app/components/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/utils/*": [
        "./app/utils/*"
      ],
      "@/screens/*": [
        "./screens/*"
      ],
      "@/store/*": [
        "./store/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}